{
  "name": "typeorm",
  "displayName": "TypeORM",
  "description": "TypeORM database explorer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:extension.typeormExplorer",
    "onCommand:extension.connectDB",
    "onCommand:extension.readConfig"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "typeormView": [
        {
          "id": "extension.typeormExplorer",
          "name": "Connections"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "typeormView",
          "title": "TypeORM Explorer",
          "icon": "media/logo.png"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.connectDB",
        "title": "TypeORM: Open Database"
      },
      {
        "command": "extension.execSQL",
        "title": "TypeORM: Execute selected SQL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/require-from-string": "^1.2.0",
    "tslint": "^5.8.0",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "pg": "^7.7.1",
    "require-from-string": "^2.0.2",
    "typeorm": "^0.2.11",
    "typescript": "^3.3.3333"
  }
}
